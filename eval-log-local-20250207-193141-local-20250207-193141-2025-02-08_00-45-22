    def test_reverse_one_to_one_multi_prefetch_related(self):
        with self.assertNumQueries(2):
            pools = 
list(Pool.objects.prefetch_related("poolstyle").order_by("pk"))
            self.assertIs(pools[1], pools[1].poolstyle.pool)
            self.assertIs(pools[2], pools[2].poolstyle.pool)
=======
    def test_reverse_one_to_one_multi_prefetch_related(self):
        with self.assertNumQueries(2):
            pools = 
list(Pool.objects.prefetch_related("poolstyle").order_by("pk"))
            self.assertIs(pools[1], pools[1].poolstyle.pool)
            self.assertIs(pools[2], pools[2].poolstyle.pool)

    def test_filtered_relation_caching(self):
        with self.assertNumQueries(2):
            tournament = Tournament.objects.annotate(
                pool_style=FilteredRelation(
                    "pool__poolstyle",
                    condition=~models.Q(pool__name__contains="1"),
                ),
            ).get(pk=self.t1.pk)
            pool = tournament.pool_set.all()[0]
            self.assertIs(tournament, pool.tournament)

    def test_wrong_select_related(self):
        with self.assertNumQueries(2):
            tournament = 
Tournament.objects.select_related("pool").get(pk=self.t1.pk)
            pool = tournament.pool_set.all()[0]
            self.assertIs(tournament, pool.tournament)
>>>>>>> REPLACE
```

