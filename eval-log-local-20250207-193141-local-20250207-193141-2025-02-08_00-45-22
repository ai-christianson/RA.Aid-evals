    def test_merge_three_way_warning(self):
        """Test merging 3+ media objects with conflicting orderings."""
        test_cases = (
            # Circular dependency
            ([1, 2], [2, 3], [3, 1]),
            # Direct conflict in ordering
            ([1, 2], [2, 3], [3, 2]),
            # Multiple conflicts
            ([1, 2], [2, 3], [3, 2], [2, 1]),
        )
        for lists in test_cases:
            with self.subTest(lists=lists):
                with self.assertWarnsRegex(MediaOrderConflictWarning,
                                         r"Detected duplicate Media files in an 
opposite order"):
                    result = Media.merge(*lists)
                # Verify all items are present regardless of order
                self.assertEqual(set(result), set(x for l in lists for x in l))
