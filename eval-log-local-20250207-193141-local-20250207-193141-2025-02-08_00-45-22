    @staticmethod
    def merge(*lists):
        """
        Merge lists while trying to keep the relative order of the elements.
        Warn if the lists have genuinely conflicting relative orders.
        """
        def _build_graph(lists):
            """Build a graph of ordering requirements from all input lists"""
            ordering = {}
            for lst in lists:
                for i, item in enumerate(lst):
                    if item not in ordering:
                        ordering[item] = {'before': set(), 'after': set()}
                    # Record items that must come after this one
                    for after in lst[i+1:]:
                        if after not in ordering:
                            ordering[after] = {'before': set(), 'after': set()}
                        ordering[item]['after'].add(after)
                        ordering[after]['before'].add(item)
            return ordering

        def _check_conflicts(graph):
            """Check for genuine ordering conflicts in the graph"""
            for item, deps in graph.items():
                # If any item must come both before and after another item,
                # that's a genuine conflict
                conflicts = deps['before'].intersection(deps['after'])
                if conflicts:
                    # Report the first found conflict
                    other = next(iter(conflicts))
                    warnings.warn(
                        'Detected duplicate Media files in an opposite order:\n'
                        '%s\n%s' % (item, other),
                        MediaOrderConflictWarning,
                    )

        def _merge_lists(lists, graph):
            """Merge while respecting ordering constraints"""
            seen = set()
            result = []
            
            # Helper to check if an item can be safely added
            def can_add(item):
                return all(
                    before in seen
                    for before in graph[item]['before']
                )
                
            # Convert lists to sets for faster lookups
            items = set().union(*lists)
            
            while items:
                # Find all items that can be added
                ready = {
                    item for item in items
                    if can_add(item)
                }
                
                if not ready:
                    # No items ready means we have a cycle
                    # Just add remaining items in their first seen order
                    ready = items
                
                # Add items in the order they first appear in any list
                for lst in lists:
                    for item in lst:
                        if item in ready:
                            result.append(item)
                            seen.add(item)
                            items.remove(item)
                            ready.remove(item)
                    if not ready:
                        break
                        
            return result

        # Handle empty input cases
        if not lists:
            return []
        if len(lists) == 1:
            return list(lists[0])

        # Build ordering graph and check for conflicts
        graph = _build_graph(lists)
        _check_conflicts(graph)
        
        # Merge preserving ordering requirements
        return _merge_lists(lists, graph)
