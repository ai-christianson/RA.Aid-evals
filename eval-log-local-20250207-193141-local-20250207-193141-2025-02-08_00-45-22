tests/forms_tests/test_media_merge.py
```python
<<<<<<< SEARCH
=======
from django.forms.widgets import Media, MediaOrderConflictWarning 
import warnings
import unittest

class MediaMergeTests(unittest.TestCase):
    def test_merge_two_lists(self):
        """Basic two-list merge should work as before"""
        list1 = [1, 2] 
        list2 = [3, 2]
        expected = [1, 3, 2]
        self.assertEqual(Media.merge(list1, list2), expected)

    def test_merge_three_lists_no_conflict(self):
        """Merging three compatible lists should work without warnings"""
        list1 = [1, 2]
        list2 = [3, 2] 
        list3 = [1, 4]
        expected = [1, 3, 2, 4]
        with warnings.catch_warnings(record=True) as w:
            warnings.simplefilter("always")
            result = Media.merge(list1, list2, list3)
            self.assertEqual(len(w), 0, "No warnings should be emitted")
        self.assertEqual(result, expected)

    def test_merge_three_lists_with_conflict(self):
        """Should warn only for actual conflicts between any lists"""
        list1 = [1, 2]
        list2 = [2, 1]
        list3 = [3, 1]
        with self.assertWarnsRegex(MediaOrderConflictWarning, 
                                 r".*\[1, 2\], \[2, 1\].*"):
            result = Media.merge(list1, list2, list3)
        self.assertTrue(1 in result)
        self.assertTrue(2 in result)
        self.assertTrue(3 in result)

    def test_merge_empty_lists(self):
        """Empty lists should be handled properly"""
        self.assertEqual(Media.merge(), [])
        self.assertEqual(Media.merge([]), [])
        self.assertEqual(Media.merge([1, 2], [], [3]), [1, 2, 3])

    def test_merge_cyclic_dependencies(self):
        """Should handle cyclic dependencies gracefully"""
        list1 = [1, 2, 3]
        list2 = [2, 3, 1]
        list3 = [3, 1, 2]
        # Should not raise exception
        result = Media.merge(list1, list2, list3)
        # All items should be present
        self.assertEqual(set(result), {1, 2, 3})
        # Order should be consistent with at least one input list
        self.assertTrue(
            result in ([1, 2, 3], [2, 3, 1], [3, 1, 2]),
            "Result must match one of the input orders"
        )

if __name__ == '__main__':
    unittest.main()
>>>>>>> REPLACE
```

